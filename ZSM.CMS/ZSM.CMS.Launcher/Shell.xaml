<Window x:Class="ZSM.CMS.Launcher.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:framworkEnum="clr-namespace:DDSoft.Common;assembly=DDSoft.Common"
        xmlns:reg="clr-namespace:Microsoft.Practices.Prism.Regions;assembly=Microsoft.Practices.Prism"
        ShowInTaskbar="True"
        WindowStartupLocation="CenterScreen"
        Title="正视美-客户管理系统" Height="700" Width="1000">

  <Grid>
    <ItemsControl reg:RegionManager.RegionName="{x:Static framworkEnum:RegionNames.MainRegion}">
      <ItemsControl.ItemsPanel>
        <ItemsPanelTemplate>
          <Grid />
        </ItemsPanelTemplate>
      </ItemsControl.ItemsPanel>
    </ItemsControl>
  </Grid>
</Window>
